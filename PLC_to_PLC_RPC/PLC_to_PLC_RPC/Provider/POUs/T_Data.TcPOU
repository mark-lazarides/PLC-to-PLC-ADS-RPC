    <Method Name="GetIOAsJson" Id="{e5726361-c4a6-49cc-8229-ef38a2fa04ec}">
      <Declaration><![CDATA[{attribute 'TcRpcEnable'}
METHOD PUBLIC GetIOAsJson : JsonDoc
VAR_INST
    cnt : INT;
END_VAR
VAR 
	jsonDocToProp : FB_JsonReadWriteDatatype;
	sizeOfJDoc : UDINT;
END_VAR
VAR_INST
	sizeOfDoc : UDINT;
	jDoc : JsonDoc;
END_VAR]]></Declaration>
      <Implementation>
        <ST><![CDATA[sizeOfDoc := SIZEOF(jDoc);
jsonDocToProp.GetJsonFromSymbol('ST_IO_Status',SIZEOF(IO.IO_Status), ADR(IO.IO_Status), sizeOfDoc, ADR(jDoc));
GetIOAsJson := jDoc;
cnt := cnt + 1;]]></ST>
      </Implementation>
    </Method>
    <Method Name="SetIOAsJson" Id="{ae2f91ce-d980-4f4a-814c-1278bb2be93a}">
      <Declaration><![CDATA[{attribute 'TcRpcEnable'}
METHOD PUBLIC SetIOAsJson : UDINT;
