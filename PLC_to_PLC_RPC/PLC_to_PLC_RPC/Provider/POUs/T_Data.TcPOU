VAR_INST
    cnt : INT;
END_VAR
VAR 
	jsonDocToProp : FB_JsonReadWriteDatatype;
	sizeOfJDoc : UDINT;
END_VAR
VAR_INST
	sizeOfDoc : UDINT;
	jDoc : JsonDoc;
	localIO : ST_IO_Status;
END_VAR]]></Declaration>
      <Implementation>
        <ST><![CDATA[SetIOAsJson := SIZEOF(jDocIn);
jDoc := jDocIn;
jsonDocToProp.SetSymbolFromJson( jDocIn, 'ST_IO_Status',SIZEOF(localIo), ADR(localIo));
cnt := cnt + 1;]]></ST>
      </Implementation>
